#!/usr/bin/env bash

source "$(dirname "$0")/utils.sh"

list_all() {
  install_or_update_python_build
  echo "$(
    $(python_build_path) --definitions
    echo 2-latest
    echo 2.1-latest
    echo 2.2-latest
    echo 2.3-latest
    echo 2.4-latest
    echo 2.5-latest
    echo 2.6-latest
    echo 2.7-latest
    echo 3-latest
    echo 3.0-latest
    echo 3.1-latest
    echo 3.2-latest
    echo 3.3-latest
    echo 3.4-latest
    echo 3.5-latest
    echo 3.6-latest
    echo 3.7-latest
    echo 3.8-latest
    echo 3.9-latest
    echo 3.10-latest
  )" | tr '\n' ' '
}

list_all
